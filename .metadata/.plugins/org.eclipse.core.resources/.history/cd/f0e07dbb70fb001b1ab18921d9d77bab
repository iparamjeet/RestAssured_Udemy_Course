package problemSolving;

public class pswdValidator {

	public static boolean passwordCheck(String pswd) {
		
		String str = pswd;
		
		if(str.length() < '8')
			return false;
		
		
		
		
		return true;
	}
	
	public static void main(String[] args) {
		
		System.out.println(passwordCheck("abcdefgh"));
	}

}
