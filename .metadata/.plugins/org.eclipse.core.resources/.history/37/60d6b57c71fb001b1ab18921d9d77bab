package problemSolving;

public class pswdValidator {

	public static boolean passwordCheck(String pswd) {
		
		String str = pswd;
		
		if(str.length() > 7)
			if(str.contains("[A-Za-z]"))
			   return true;
			
		
		
	    return false;
		
	}
	
	
	
	public static void main(String[] args) {
		
		System.out.println(passwordCheck("abcdefghi"));
	}

}
